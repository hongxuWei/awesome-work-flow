{
  "manifest_version": 2,
  "name": "Awesome Work Flow",
  "version": "1.0.0",
  "description": "Awesome Work Flow - Extension",
  "icons": {
    "16": "statics/icon.png",
    "48": "statics/icon.png",
    "128": "statics/icon.png"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["js/content.js"],
    "run_at": "document_end"
  }],
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "defailt_icon": "statics/icon.png",
    "default_title": "Awesome Work Flow",
    "default_popup": "index.html"
  },
  "chrome_url_overrides": { "newtab": "index.html#newtab" },
  "permissions": [
    "tabs",
    "webRequest",
    "contextMenus",
    "notifications",
    "webRequestBlocking",
    "bookmarks",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "homepage_url": "https://hongxuWei.github.io"
}
